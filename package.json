        "sass-loader": "^13.0.0",
        "sinon": "^14.0.0",
        "style-loader": "^3.3.0",
        "terser-webpack-plugin": "^5.3.1",
        "timezone-mock": "^1.3.1",
        "webpack": "^5.72.1",
        "webpack-bundle-analyzer": "^4.4.2",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.9.0",
