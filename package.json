        "@commitlint/cli": "^17.0.0",
        "@commitlint/config-conventional": "^17.0.0",
        "@release-it/conventional-changelog": "^5.0.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@types/sinon": "^10.0.11",
        "babel-loader": "^8.2.5",
        "copy-webpack-plugin": "^11.0.0",
        "copyfiles": "^2.4.1",
        "core-js": "3.22.5",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "eslint": "^8.15.0",
        "eslint-config-prettier": "^8.5.0",
